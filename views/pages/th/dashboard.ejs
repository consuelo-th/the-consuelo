      <!-- submit certifications Modal content -->
      <div
        class="bg-white rounded-2xl p-8 my-0 mx-auto w-3/4 md:w-1/3 h-auto modal-content"
      >
        <div class="space-y-3 w-fit m-auto text-center">
          <img src="/images/sucessful-icon.svg" alt="" class="m-auto" />
          <h2>Successfully submitted</h2>
          <p class="close cursor-pointer font-medium text-sm text-gray-50">
            Continue
          </p>
        </div>
      </div>
    </div>

    <!-- contact Modal  -->
    <div
      id="contact_modal"
      class="modal hidden fixed z-1 left-0 top-0 w-full h-full overflow-auto pt-32 bg-opacity-20 bg-secondary-100"
    >
      <!-- contact Modal content -->
      <div
        class="bg-white rounded-2xl p-8 my-0 mx-auto w-11/12 md:w-3/5 h-auto modal-content"
      >
        <button class="pb-15 pr-10" id="back_btn3">
          <i class="fa-solid fa-arrow-left"></i>Complete Profile
        </button>
        <div class="border rounded border-solid border-primary-40 pt-4">
          <div class="pl-8 pb-14">
            <h2>Phone Number & Email</h2>
            <p>Input a valid phone number</p>
          </div>
          <form class="px-8 pb-14 w-full space-y-4" action="">
            <input
              class="w-full p-4 border rounded"
              type="tel"
              name="phonenumber"
              id="phonenumber"
              placeholder="Phone number"
            /><br />
            <input
              class="w-full p-4 border rounded"
              type="email"
              name="email"
              id="email"
              placeholder="john@example.com"
            />
          </form>
        </div>
        <div class="mt-6">
          <button
            class="bg-primary-50 text-white px-8 md:px-20 py-4 text-center rounded"
            id="submit_contact"
          >
            Update
          </button>
        </div>
      </div>
